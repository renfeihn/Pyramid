{
  "customBaseDir": "",
  "customDefaultAtt": {
    "type": "dateFile",
    "absolute": true,
    "alwaysIncludePattern": true
  },
  "appenders": [
    {
      "type": "console"
    },
    {
      "pattern": "debug/yyyyMMddhh.log",
      "category": "logDebug"
    },
    {
      "pattern": "info/yyyyMMddhh.log",
      "category": "logInfo"
    },
    {
      "pattern": "warn/yyyyMMddhh.log",
      "category": "logWarn"
    },
    {
      "pattern": "error/yyyyMMddhh.log",
      "category": "logErr"
    }
  ],
  "replaceConsole": true,
  "levels": {
    "logDebug": "DEBUG",
    "logInfo": "INFO",
    "logWarn": "WARN",
    "logErr": "ERROR"
  }
}